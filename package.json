{
  "name": "aws-sdk-wrap",
  "version": "0.0.0-development",
  "description": "Wrapper around aws-sdk",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "npx babel src --out-dir lib --copy-files --include-dotfiles --config-file ./.babelrc",
    "build-clean": "npm run clean && npm run build",
    "test-simple": "nyc mocha \"./test/**/*.spec.js\"",
    "test": "npm run clean && npm run gardener && npm run test-simple",
    "coveralls": "node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
    "semantic-release": "npm run build-clean && npx semantic-release",
    "gardener": "node gardener"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blackflux/aws-sdk-wrap.git"
  },
  "keywords": [
    "aws-sdk"
  ],
  "author": "Lukas Siemon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blackflux/aws-sdk-wrap/issues"
  },
  "homepage": "https://github.com/blackflux/aws-sdk-wrap#readme",
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/register": "7.4.4",
    "@blackflux/eslint-plugin-rules": "1.0.18",
    "@blackflux/robo-config-plugin": "1.10.4",
    "aws-sdk": "2.464.0",
    "babel-eslint": "10.0.1",
    "chai": "4.2.0",
    "coveralls": "3.0.3",
    "eslint": "5.16.0",
    "js-gardener": "2.0.34",
    "nock": "10.0.6",
    "nyc": "14.1.1",
    "semantic-release": "15.13.12"
  },
  "peerDependencies": {
    "aws-sdk": "^2.296.0"
  },
  "nyc": {
    "tempDir": "./coverage/.nyc_output",
    "report-dir": "./coverage",
    "check-coverage": true,
    "per-file": false,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "include": [
      "**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "@babel/register"
    ],
    "extension": [],
    "cache": true,
    "all": true,
    "babel": true,
    "exclude": [
      "gardener.js",
      "node_modules/*",
      "coverage/*",
      "lib/*"
    ]
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/blackflux/aws-sdk-wrap/blob/master/LICENSE"
    }
  ],
  "engines": {
    "node": ">= 8.10"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "lodash.get": "4.4.2"
  }
}
